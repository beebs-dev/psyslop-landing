# Copy this file to `.env` and fill in your credentials.
# DigitalOcean Spaces is S3-compatible, so AWS-style env vars work.

# Required
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# Optional
AWS_SESSION_TOKEN=

# Spaces config (defaults match your request)
SPACES_ENDPOINT=https://sfo3.digitaloceanspaces.com
SPACES_BUCKET=slop
AWS_REGION=sfo3

# VIDEO_SORT_BY=id (default) or created_at
VIDEO_SORT_BY=id